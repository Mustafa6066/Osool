{
    "buildCommand": null,
    "outputDirectory": "public",
    "devCommand": null,
    "installCommand": "npm install --legacy-peer-deps",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "headers": [
        {
            "source": "/assets/js/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}